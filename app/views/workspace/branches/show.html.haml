- branch ||= @branch 
- commits ||= @commits 

%h3 Branch #{branch.name}

%table.commits.table.table-striped
  %thead
  %tbody
    = render partial: 'commit_row', collection: commits, as: :commit 

= paginate commits, theme: 'default'

