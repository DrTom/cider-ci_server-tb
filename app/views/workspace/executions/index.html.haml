- executions ||= @executions
- link_params ||= @link_params


- cache @execution_cache_signatures.map{|cs| [cs.stats_signature,cs.commits_signature,cs.branches_signature]} do |cache_tag|

  #reload-page.executions-page{data: {cache_tag: cache_tag, reload_timeout: 30}}

    .pull-right=render 'index_filter_form'

    %h1 
      %i.icon-cog
      Executions

    .clearfix

    = render partial: "executions_table", executions: executions, link_params: link_params

    = paginate executions, theme: 'default'


